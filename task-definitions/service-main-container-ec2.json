[
  {
     "cpu":${cpu},
     "image":"${service_image}",
     "memory":${memory},
     "name":"${service_name}",
     "networkMode":"awsvpc",
     "essential":true,
     "logConfiguration":{
        "logDriver":"awslogs",
        "options":{
           "awslogs-group":"${log_group_name}",
           "awslogs-region":"${region}",
           "awslogs-stream-prefix":"${service_name}"
        }
     },
     "portMappings":[
        {
           "containerPort":${service_port},
           "hostPort":${service_port}
        }
     ],
     "environment" : ${envvars},
     "secrets" : ${secrets_task_definition},
     "entryPoint": ${entry_point},
     "command": ${command},
     "systemControls": [
      {
         "namespace": "net.core.somaxconn",
         "value": "${unix_max_connection}"
      }
   ]
  }
]
